{
  "name": "eleventy-high-performance-blog",
  "version": "0.0.7",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "repository": "github:nicjay/njordan.dev",
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "scripts": {
    "build-ci": "npm run js-build && eleventy && npm run test",
    "build": "npm run clean && npm run build-ci",
    "watch": "concurrently \"npm run serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "npm run clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "run-script-os",
    "clean:default": "rm -rf _site/posts/*",
    "clean:win32": "if exist _site\\posts\\ rd /s /q _site\\posts"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-img": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.5",
    "cssnano": "^5.1.7",
    "jsdom": "^19.0.0",
    "luxon": "^2.3.2",
    "markdown-it": "^13.0.0",
    "markdown-it-anchor": "^8.6.2",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "prismjs": "^1.28.0",
    "rollup-plugin-terser": "^7.0.2",
    "run-script-os": "^1.1.6",
    "shorthash": "0.0.2",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "concurrently": "^7.1.0",
    "csp-hash-generator": "0.0.3",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "mocha": "^9.2.2",
    "phin": "^3.6.1",
    "querystring": "^0.2.0",
    "rollup": "^2.70.2",
    "touch": "^3.1.0",
    "twitter": "^1.7.1"
  }
}
